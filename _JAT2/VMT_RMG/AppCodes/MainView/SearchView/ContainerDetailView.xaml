                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="Btn_Container_Target" Margin="10,5,10,0" Style="{DynamicResource SkinButtonStyle_Without_FontSize}" Content="MSKU3645190" FontSize="35" Foreground="#FFFFFFFF" Grid.Column="0"/>
