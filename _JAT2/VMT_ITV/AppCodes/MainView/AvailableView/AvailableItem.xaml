	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="VMT_ITV.AvailableItem"
	x:Name="UserControl"
	d:DesignWidth="147" d:DesignHeight="126" Width="157" Height="137">

	<Grid x:Name="Grid_AvailableItem" Margin="4" MouseLeave="Grid_Available_MouseLeave" MouseLeftButtonDown="Grid_Available_MouseLeftButtonDown" MouseLeftButtonUp="Grid_Available_MouseLeftButtonUp">
