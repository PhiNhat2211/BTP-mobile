<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="VMT_RMG.AvailableControl"
	x:Name="UserControl"
	d:DesignWidth="250" d:DesignHeight="180" Width="252" Height="184" 
	Loaded="UserControl_Loaded">

    <Grid x:Name="LayoutRoot">
        <Button x:Name="Button_Available" Margin="4" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{DynamicResource SkinButtonStyle_With_Content}"
                FontFamily="/VMT_RTG;component/Fonts/#Neo Sans Pro Medium" FontSize="32" Foreground="{DynamicResource TextBox_Foreground_10}" Click="Button_Available_Click">
        </Button>
    </Grid>
</UserControl>