            <Button x:Name="Button_FullPad_done_2" Content="DONE" Margin="5" HorizontalAlignment="Left" Style="{DynamicResource SkinButtonStyle}" Focusable="False" VerticalAlignment="Top" Foreground="White" BorderThickness="1" Click="Button_keypad_done_Click" Grid.Column="7" Grid.Row="3"  Grid.ColumnSpan="3"/>
        </Grid>
    </Grid>
</UserControl>